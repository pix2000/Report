    9.
    #include<stdio.h>
    struct user{
    int a;
    char n[20];
    char m[40];
    }
    int main(){
    struct user she = {18,"Jenny",,"Jenny@naver.com};
    struct user he = {19,"Jack",,"Jack@naver.com};
    printf("%d%s%s\n", she.a, she.n, she.m);
    printf("%d%s%s\n", he.a, he.n, he.m);
    }
    10.
    #include <stdio.h>

struct user {
   int a;
   char n[20];
   char n[40];
}
  we[2] = { {18, "Jenny", "Jenny@naver.com"}, {19, "Jack","Jack@naver.com"} };
  int main() {
   struct user* pr;
   pr = we;
   printf("%d %s\n", pr->a, pr->n, pr->m);
   pr++;
   printf("%d %s\n", pr->a, pr->n, pr->m);
   
}
    
